{
  "name": "turbo-template",
  "version": "0.0.1",
  "description": "turborepo",
  "private": true,
  "scripts": {
    "dev": "turbo dev",
    "dev:vite-vue3": "turbo dev --filter @apps/vite-vue3",
    "build": "turbo build",
    "check-types": "turbo check-types",
    "lint": "turbo lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "clean": "pnpm turbo run clean && rimraf node_modules"
  },
  "keywords": [
    "turborepo"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "husky": "^9.0.11",
    "rimraf": "^5.0.8",
    "turbo": "^2.0.6",
    "typescript": "^5.5.3",
    "unbuild": "^2.0.0",
    "vitest": "^1.6.0"
  },
  "packageManager": "pnpm@9.0.0",
  "engines": {
    "node": ">=16"
  }
}
